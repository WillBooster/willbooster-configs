{
  "name": "@willbooster/shared",
  "version": "0.0.0-semantically-released",
  "private": true,
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "main": "./src",
  "scripts": {
    "check-all-for-ai": "yarn check-for-ai && yarn test",
    "check-for-ai": "yarn install > /dev/null && yarn format > /dev/null 2> /dev/null || true && yarn typecheck && yarn lint-fix --quiet",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,java,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test{-,/}fixtures/**\" || true",
    "test": "yarn lint",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/eslint": "9.6.1",
    "@types/micromatch": "4.0.10",
    "@willbooster/eslint-config-ts": "workspace:*",
    "@willbooster/prettier-config": "workspace:*",
    "eslint": "10.0.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-sort-class-members": "1.21.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-unicorn": "63.0.0",
    "eslint-plugin-unused-imports": "4.4.1",
    "globals": "17.3.0",
    "lint-staged": "16.2.7",
    "micromatch": "4.0.8",
    "prettier": "3.8.1",
    "prettier-plugin-java": "2.8.1",
    "sort-package-json": "3.6.1",
    "typescript": "5.9.3",
    "typescript-eslint": "8.56.0"
  }
}
