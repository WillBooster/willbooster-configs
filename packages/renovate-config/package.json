{
  "name": "@willbooster/renovate-config",
  "version": "6.2.1",
  "description": "A renovate config for WillBooster projects",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "scripts": {
    "cleanup": "yarn format",
    "format": "sort-package-json && yarn prettify",
    "prettify": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\" --color"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@willbooster/prettier-config": "^6.2.1",
    "prettier": "2.2.1",
    "sort-package-json": "1.49.0"
  },
  "gitHead": "865a18e97886b4051f555430f8fc0c89bd364001",
  "renovate-config": {
    "default": {
      "extends": [
        "config:base",
        ":automergeBranch",
        ":automergeMinor",
        ":maintainLockFilesWeekly",
        ":rebaseStalePrs",
        ":semanticCommits",
        "group:allNonMajor",
        "group:linters",
        "group:monorepos",
        "group:test"
      ],
      "schedule": [
        "before 10am on Saturday"
      ],
      "timezone": "Asia/Tokyo",
      "packageRules": [
        {
          "groupName": "willbooster-config",
          "packagePatterns": [
            "^@willbooster/.*config"
          ]
        },
        {
          "packageNames": [
            "husky"
          ],
          "enabled": false
        }
      ]
    }
  }
}
