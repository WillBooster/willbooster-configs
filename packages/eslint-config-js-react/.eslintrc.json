{
  "extends": ["plugin:react/recommended", "plugin:react-hooks/recommended", "@willbooster/eslint-config-js"],
  "rules": {
    "react/jsx-sort-props": [
      "error",
      {
        "callbacksLast": true,
        "shorthandFirst": true,
        "reservedFirst": true
      }
    ],
    "react/prop-types": "off"
  },
  "settings": {
    "import/core-modules": ["react-dom"],
    "react": {
      "version": "detect"
    }
  }
}
