{
  "name": "@willbooster/babel-configs",
  "version": "0.0.0-semantically-released",
  "description": "Babel configs for WillBooster projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/WillBooster/willbooster-configs.git",
    "directory": "packages/babel-configs"
  },
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "files": [
    "babel.*.cjs"
  ],
  "scripts": {
    "check-all-for-ai": "yarn check-for-ai && yarn test",
    "check-for-ai": "yarn install > /dev/null && yarn format > /dev/null 2> /dev/null || true",
    "cleanup": "yarn format",
    "format": "sort-package-json && yarn prettify",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,java,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test{-,/}fixtures/**\" || true"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/babel__core": "7.20.5",
    "@willbooster/prettier-config": "^10.0.0",
    "core-js": "3.47.0",
    "lint-staged": "16.2.7",
    "prettier": "3.7.4",
    "prettier-plugin-java": "2.7.7",
    "sort-package-json": "3.5.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
