{
  "name": "@willbooster/configs",
  "version": "1.0.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/WillBooster/willbooster-configs.git"
  },
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn && yarn lerna bootstrap --use-workspaces",
    "format": "yarn prettier && yarn sort-all-package-json",
    "prettier": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,jsx,md,scss,ts,tsx,vue,yaml,yml}\"",
    "publish-all": "npm login && npm config set access public && yarn lerna publish",
    "sort-all-package-json": "yarn sort-package-json && yarn lerna run sort-package-json"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {},
  "devDependencies": {
    "husky": "3.0.9",
    "lerna": "3.18.4",
    "lint-staged": "9.4.3",
    "prettier": "1.19.1",
    "sort-package-json": "1.23.1"
  }
}
