{
  "name": "@willbooster/gen-configs",
  "version": "0.0.0-development",
  "private": false,
  "description": "A config generator for WillBooster projects",
  "license": "Apache-2.0",
  "author": "WillBooster LLC",
  "files": [
    "/bin",
    "/dist"
  ],
  "main": ".eslintrc.json",
  "bin": {
    "gen-configs": "./bin/run"
  },
  "scripts": {
    "build": "webpack --config webpack.config --mode production",
    "build-dev": "webpack --config webpack.config --mode development",
    "format": "yarn prettier && yarn sort-package-json && yarn lint-fix",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "yarn lint --fix",
    "prepack": "rimraf dist && yarn build",
    "prettier": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,jsx,md,scss,ts,tsx,yaml,yml}\"",
    "sort-package-json": "sort-package-json",
    "start": "./bin/run",
    "test": "jest",
    "typecheck": "tsc --noEmit"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@oclif/command": "1.5.19",
    "@oclif/config": "1.13.3",
    "@oclif/plugin-help": "2.2.1",
    "deepmerge": "4.2.2",
    "fs-extra": "8.1.0",
    "glob": "7.1.4",
    "js-yaml": "3.13.1",
    "node-fetch": "2.6.0",
    "tslib": "1.10.0"
  },
  "devDependencies": {
    "@types/fs-extra": "8.0.1",
    "@types/glob": "7.1.1",
    "@types/js-yaml": "3.12.1",
    "@types/node-fetch": "2.5.3",
    "ts-node": "8.5.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "oclif": {
    "bin": "gen-configs"
  }
}
