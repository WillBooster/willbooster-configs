{
  "name": "@willbooster/renovate-config",
  "version": "0.1.5",
  "private": false,
  "description": "A renovate config for WillBooster projects",
  "license": "Apache-2.0",
  "author": "WillBooster LLC",
  "scripts": {
    "sort-package-json": "sort-package-json"
  },
  "gitHead": "dd9f883adfe583f565540544cbb54a11c257ade3",
  "renovate-config": {
    "default": {
      "extends": [
        "config:base"
      ],
      "automergeType": "branch-push",
      "prCreation": "immediate",
      "rebaseStalePrs": true,
      "semanticCommits": true,
      "timezone": "Asia/Tokyo",
      "dependencies": {
        "automerge": true,
        "major": {
          "automerge": false
        }
      },
      "devDependencies": {
        "automerge": true
      },
      "packageRules": [
        {
          "groupName": "willbooster-config",
          "packagePatterns": [
            "^@willbooster/.*config"
          ]
        }
      ]
    }
  }
}
