{
  "name": "@willbooster/eslint-config-ts-react",
  "version": "0.0.0-semantically-released",
  "description": "A ESLint config for TypeScript projects using React",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": ".eslintrc.json",
  "files": [
    ".eslintrc.json"
  ],
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn lint",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@willbooster/eslint-config-ts": "^10.5.1"
  },
  "devDependencies": {
    "@types/eslint": "8.44.7",
    "@types/micromatch": "4.0.5",
    "@types/node": "20.9.1",
    "@types/react": "18.2.37",
    "@typescript-eslint/eslint-plugin": "6.11.0",
    "@typescript-eslint/parser": "6.11.0",
    "@willbooster/prettier-config": "^9.0.1",
    "eslint": "8.53.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-class-members": "1.19.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "49.0.0",
    "lint-staged": "15.1.0",
    "micromatch": "4.0.5",
    "prettier": "3.1.0",
    "react": "18.2.0",
    "sort-package-json": "2.6.0",
    "typescript": "5.2.2",
    "use-immer": "0.9.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=5",
    "@typescript-eslint/parser": ">=5",
    "@willbooster/prettier-config": ">=6",
    "eslint": ">=8",
    "eslint-config-prettier": ">=8",
    "eslint-import-resolver-typescript": ">=2",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=4",
    "eslint-plugin-sort-class-members": ">=1.14",
    "eslint-plugin-sort-destructure-keys": ">=1.4",
    "typescript": ">=4"
  },
  "publishConfig": {
    "access": "public"
  }
}
