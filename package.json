{
  "name": "willbooster-configs",
  "private": true,
  "license": "Apache-2.0",
  "author": "WillBooster LLC",
  "workspaces": [
    "projects/*"
  ],
  "scripts": {
    "bootstrap": "yarn lerna bootstrap --use-workspaces",
    "format": "yarn prettier && yarn sort-all-package-json",
    "prettier": "prettier --write {*,**/*}.{css,htm,html,js,json,md,scss,yaml,yml} && yarn sort-all-package-json",
    "sort-all-package-json": "yarn sort-package-json && yarn lerna run sort-package-json"
  },
  "devDependencies": {
    "husky": "2.4.1",
    "lerna": "3.15.0",
    "lint-staged": "8.2.1",
    "prettier": "1.17.1",
    "renovate-config-willbooster": "0.0.4",
    "sort-package-json": "1.22.1"
  }
}
