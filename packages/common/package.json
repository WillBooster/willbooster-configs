{
  "name": "@willbooster/common",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn lint"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/micromatch": "4.0.2",
    "@typescript-eslint/eslint-plugin": "5.10.1",
    "@typescript-eslint/parser": "5.10.1",
    "@willbooster/eslint-config-ts": "workspace:*",
    "@willbooster/prettier-config": "workspace:*",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-sort-class-members": "1.14.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "lint-staged": "12.3.2",
    "micromatch": "4.0.4",
    "prettier": "2.5.1",
    "sort-package-json": "1.53.1"
  },
  "peerDependencies": {
    "@willbooster/prettier-config": ">=6"
  }
}
