{
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:import/warnings", "prettier"],
  "rules": {
    "eqeqeq": "warn",
    "no-console": "off", // allow `console.log()`
    "import/newline-after-import": "error",
    "import/no-duplicates": "error",
    "import/order": [
      "error",
      {
        "newlines-between": "always",
        "alphabetize": {
          "order": "asc"
        }
      }
    ]
  }
}
