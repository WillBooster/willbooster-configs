{
  "name": "@willbooster/eslint-config-ts",
  "version": "6.3.0",
  "description": "A ESLint config for TypeScript projects",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": ".eslintrc.json",
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "prettify": "prettier --color --write \"**/{.*/,}*.{css,htm,html,js,json,json5,jsx,md,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "eslint __tests__",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{js,jsx,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/eslint": "7",
    "@types/micromatch": "^4",
    "@types/node": "14.17.12",
    "@willbooster/prettier-config": "^6.2.1",
    "eslint": "7.32.0",
    "micromatch": "4.0.4",
    "prettier": "2.3.2",
    "sort-package-json": "1.50.0",
    "typescript": "4.3.5",
    "use-immer": "0.6.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=4",
    "@typescript-eslint/parser": ">=4",
    "@willbooster/prettier-config": ">=5",
    "eslint": ">=7",
    "eslint-config-prettier": ">=8",
    "eslint-plugin-import": ">=2",
    "typescript": ">=4"
  },
  "gitHead": "865a18e97886b4051f555430f8fc0c89bd364001"
}
