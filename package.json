{
  "name": "@willbooster/configs",
  "private": true,
  "license": "Apache-2.0",
  "author": "WillBooster LLC",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "yarn lerna bootstrap --use-workspaces",
    "format": "yarn prettier && yarn sort-all-package-json",
    "prettier": "prettier --write {,*/}**/*.{css,htm,html,js,json,md,scss,yaml,yml}",
    "sort-all-package-json": "yarn sort-package-json && yarn lerna run sort-package-json"
  },
  "devDependencies": {
    "husky": "3.0.9",
    "lerna": "3.18.2",
    "lint-staged": "9.4.2",
    "prettier": "1.18.2",
    "sort-package-json": "1.22.1"
  }
}
