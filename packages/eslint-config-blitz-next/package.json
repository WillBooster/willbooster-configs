{
  "name": "@willbooster/eslint-config-blitz-next",
  "version": "0.0.0-semantically-released",
  "description": "A ESLint config for Blitz.js projects using Next.js",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": ".eslintrc.js",
  "files": [
    ".eslintrc.js"
  ],
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{scripts,src,tests}/**/*.{cjs,cts,js,jsx,mjs,mts,ts,tsx}\"",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn lint",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@willbooster/eslint-config-ts": "^10.0.8"
  },
  "devDependencies": {
    "@blitzjs/next": "2.0.0-beta.33",
    "@types/eslint": "8.44.2",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.17.18",
    "@types/react": "18.2.22",
    "@typescript-eslint/eslint-plugin": "6.7.2",
    "@typescript-eslint/parser": "6.7.2",
    "@willbooster/prettier-config": "^9.0.1",
    "eslint": "8.49.0",
    "eslint-config-next": "13.5.2",
    "eslint-config-prettier": "9.0.0",
    "eslint-import-resolver-typescript": "3.6.1",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-class-members": "1.18.0",
    "eslint-plugin-sort-destructure-keys": "1.5.0",
    "eslint-plugin-unicorn": "48.0.1",
    "lint-staged": "14.0.1",
    "micromatch": "4.0.5",
    "prettier": "3.0.3",
    "react": "18.2.0",
    "sort-package-json": "2.5.1",
    "typescript": "5.2.2",
    "use-immer": "0.9.0"
  },
  "peerDependencies": {
    "@blitzjs/next": ">=2.0.0-beta.19",
    "@typescript-eslint/eslint-plugin": ">=5",
    "@typescript-eslint/parser": ">=5",
    "@willbooster/prettier-config": ">=6",
    "eslint": ">=8",
    "eslint-config-next": ">=13",
    "eslint-config-prettier": ">=8",
    "eslint-import-resolver-typescript": ">=2",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-sort-class-members": ">=1.14",
    "eslint-plugin-sort-destructure-keys": ">=1.4",
    "typescript": ">=4"
  },
  "publishConfig": {
    "access": "public"
  }
}
