{
  "name": "@willbooster/eslint-config-ts-react",
  "version": "8.0.0",
  "description": "A ESLint config for TypeScript projects using React",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": ".eslintrc.json",
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn lint",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@willbooster/eslint-config-ts": "workspace:*"
  },
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/micromatch": "4.0.2",
    "@types/node": "16.11.26",
    "@types/react": "17.0.39",
    "@typescript-eslint/eslint-plugin": "5.12.1",
    "@typescript-eslint/parser": "5.12.1",
    "@willbooster/prettier-config": "workspace:*",
    "eslint": "8.9.0",
    "eslint-config-prettier": "8.4.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-react": "7.29.0",
    "eslint-plugin-react-hooks": "4.3.0",
    "eslint-plugin-sort-class-members": "1.14.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "lint-staged": "12.3.4",
    "micromatch": "4.0.4",
    "prettier": "2.5.1",
    "react": "17.0.2",
    "sort-package-json": "1.54.0",
    "typescript": "4.5.5",
    "use-immer": "0.6.0"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=5",
    "@typescript-eslint/parser": ">=5",
    "@willbooster/prettier-config": ">=6",
    "eslint": ">=8",
    "eslint-config-prettier": ">=8",
    "eslint-import-resolver-typescript": ">=2",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=4",
    "eslint-plugin-sort-class-members": ">=1.14",
    "eslint-plugin-sort-destructure-keys": ">=1.4",
    "typescript": ">=4"
  },
  "gitHead": "865a18e97886b4051f555430f8fc0c89bd364001"
}
