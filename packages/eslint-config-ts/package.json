{
  "name": "@willbooster/eslint-config-ts",
  "version": "0.0.0-semantically-released",
  "description": "A ESLint flat config for TypeScript projects",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "main": "eslint.config.js",
  "files": [
    "eslint.config.js"
  ],
  "scripts": {
    "check-all-for-ai": "yarn check-for-ai && yarn test",
    "check-for-ai": "yarn install > /dev/null && yarn format > /dev/null 2> /dev/null || true && yarn lint-fix --quiet && yarn typecheck",
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,java,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test{-,/}fixtures/**\" || true",
    "test": "yarn lint",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@willbooster/eslint-config-js": "^11.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.37.0",
    "@types/eslint": "9.6.1",
    "@types/micromatch": "4.0.9",
    "@types/node": "24.7.1",
    "@willbooster/prettier-config": "^10.0.0",
    "eslint": "9.37.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-import-resolver-typescript": "4.4.4",
    "eslint-plugin-import-x": "4.16.1",
    "eslint-plugin-sort-class-members": "1.21.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-unicorn": "61.0.2",
    "eslint-plugin-unused-imports": "4.2.0",
    "globals": "16.4.0",
    "lint-staged": "16.2.3",
    "micromatch": "4.0.8",
    "prettier": "3.6.2",
    "prettier-plugin-java": "2.7.5",
    "sort-package-json": "3.4.0",
    "typescript": "5.9.3",
    "typescript-eslint": "8.46.0",
    "use-immer": "0.11.0"
  },
  "peerDependencies": {
    "@eslint/js": ">=9",
    "eslint": ">=9",
    "eslint-config-flat-gitignore": ">=2.1",
    "eslint-config-prettier": ">=10",
    "eslint-import-resolver-typescript": ">=3",
    "eslint-plugin-import-x": ">=4",
    "eslint-plugin-sort-class-members": ">=1.21",
    "eslint-plugin-sort-destructure-keys": ">=2",
    "eslint-plugin-unicorn": ">=57",
    "eslint-plugin-unused-imports": ">=4",
    "globals": ">=16",
    "typescript": ">=5",
    "typescript-eslint": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
