{
  "name": "@willbooster/common",
  "version": "1.0.0",
  "private": true,
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__,scripts}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn lint",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/eslint": "8.4.1",
    "@types/micromatch": "4.0.2",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "@willbooster/prettier-config": "workspace:*",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "2.7.1",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-sort-class-members": "1.14.1",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "lint-staged": "12.3.7",
    "micromatch": "4.0.5",
    "prettier": "2.6.2",
    "sort-package-json": "1.55.0",
    "typescript": "4.6.3"
  },
  "peerDependencies": {
    "@willbooster/prettier-config": ">=6"
  }
}
