{
  "name": "@willbooster/renovate-config",
  "version": "2.2.0",
  "private": false,
  "description": "A renovate config for WillBooster projects",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "scripts": {
    "format": "yarn prettier && yarn sort-package-json",
    "prettier": "prettier --write \"**/{.*/,}*.{css,htm,html,js,json,jsx,md,scss,ts,tsx,vue,yaml,yml}\"",
    "sort-package-json": "sort-package-json",
    "test": "jest"
  },
  "prettier": "@willbooster/prettier-config",
  "gitHead": "865a18e97886b4051f555430f8fc0c89bd364001",
  "renovate-config": {
    "default": {
      "extends": [
        ":base",
        ":automergeBranch",
        ":maintainLockFilesWeekly",
        ":rebaseStalePrs",
        ":semanticCommits",
        "group:allNonMajor",
        "group:linters",
        "group:monorepos",
        "group:test",
        "schedule:weekly"
      ],
      "timezone": "Asia/Tokyo",
      "dependencies": {
        "automerge": true,
        "major": {
          "automerge": false
        }
      },
      "devDependencies": {
        "automerge": true
      },
      "packageRules": [
        {
          "groupName": "willbooster-config",
          "packagePatterns": [
            "^@willbooster/.*config"
          ]
        }
      ]
    }
  }
}
