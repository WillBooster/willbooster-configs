{
  "name": "@willbooster/eslint-config-blitz-next",
  "version": "0.0.0-semantically-released",
  "description": "A ESLint config for Blitz.js projects using Next.js",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": ".eslintrc.js",
  "files": [
    ".eslintrc.js"
  ],
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__,scripts}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn lint",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@willbooster/eslint-config-ts": "^10.0.2"
  },
  "devDependencies": {
    "@blitzjs/next": "2.0.0-beta.19",
    "@types/eslint": "8.4.10",
    "@types/micromatch": "4.0.2",
    "@types/node": "18.11.10",
    "@types/react": "18.0.26",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "@willbooster/prettier-config": "^9.0.1",
    "eslint": "8.29.0",
    "eslint-config-next": "13.0.6",
    "eslint-config-prettier": "8.5.0",
    "eslint-import-resolver-typescript": "3.5.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-react": "7.31.11",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-sort-class-members": "1.16.0",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-unicorn": "45.0.1",
    "lint-staged": "13.0.4",
    "micromatch": "4.0.5",
    "prettier": "2.8.0",
    "react": "18.2.0",
    "sort-package-json": "2.1.0",
    "typescript": "4.9.3",
    "use-immer": "0.8.1"
  },
  "peerDependencies": {
    "@blitzjs/next": ">=2.0.0-beta.19",
    "@typescript-eslint/eslint-plugin": ">=5",
    "@typescript-eslint/parser": ">=5",
    "@willbooster/prettier-config": ">=6",
    "eslint": ">=8",
    "eslint-config-next": ">=13",
    "eslint-config-prettier": ">=8",
    "eslint-import-resolver-typescript": ">=2",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=4",
    "eslint-plugin-sort-class-members": ">=1.14",
    "eslint-plugin-sort-destructure-keys": ">=1.4",
    "typescript": ">=4"
  },
  "publishConfig": {
    "access": "public"
  }
}
