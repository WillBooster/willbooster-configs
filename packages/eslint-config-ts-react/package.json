{
  "name": "@willbooster/eslint-config-ts-react",
  "version": "0.0.0-semantically-released",
  "description": "A ESLint flat config for TypeScript projects using React",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "type": "module",
  "main": "eslint.config.js",
  "files": [
    "eslint.config.js"
  ],
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color",
    "lint-fix": "yarn lint --fix --rule \"{ react-hooks/exhaustive-deps: 0 }\"",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsonc,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn lint",
    "typecheck": "tsc --noEmit --Pretty"
  },
  "prettier": "@willbooster/prettier-config",
  "dependencies": {
    "@willbooster/eslint-config-js-react": "^11.2.0",
    "@willbooster/eslint-config-ts": "^11.2.0"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@types/eslint": "9.6.1",
    "@types/micromatch": "4.0.9",
    "@types/node": "22.15.2",
    "@types/react": "19.1.2",
    "@willbooster/prettier-config": "^10.0.0",
    "eslint": "9.24.0",
    "eslint-config-flat-gitignore": "2.1.0",
    "eslint-config-prettier": "10.1.2",
    "eslint-import-resolver-typescript": "4.3.4",
    "eslint-plugin-import-x": "4.11.0",
    "eslint-plugin-react": "7.37.5",
    "eslint-plugin-react-hooks": "5.2.0",
    "eslint-plugin-sort-class-members": "1.21.0",
    "eslint-plugin-sort-destructure-keys": "2.0.0",
    "eslint-plugin-unicorn": "58.0.0",
    "eslint-plugin-unused-imports": "4.1.4",
    "globals": "16.0.0",
    "lint-staged": "15.5.1",
    "micromatch": "4.0.8",
    "prettier": "3.5.3",
    "react": "19.1.0",
    "sort-package-json": "3.0.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "use-immer": "0.11.0"
  },
  "peerDependencies": {
    "@eslint/js": ">=9",
    "eslint": ">=9",
    "eslint-config-flat-gitignore": ">=2.1",
    "eslint-config-prettier": ">=10",
    "eslint-import-resolver-typescript": ">=3",
    "eslint-plugin-import-x": ">=4",
    "eslint-plugin-react": ">=7",
    "eslint-plugin-react-hooks": ">=5",
    "eslint-plugin-sort-class-members": ">=1.21",
    "eslint-plugin-sort-destructure-keys": ">=2",
    "eslint-plugin-unicorn": ">=57",
    "eslint-plugin-unused-imports": ">=4",
    "globals": ">=16",
    "typescript": ">=5",
    "typescript-eslint": ">=8"
  },
  "publishConfig": {
    "access": "public"
  }
}
