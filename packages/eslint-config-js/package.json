{
  "name": "@willbooster/eslint-config-js",
  "version": "0.0.0-semantically-released",
  "description": "A ESLint config for JavaScript projects",
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "main": ".eslintrc.json",
  "files": [
    ".eslintrc.json"
  ],
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify",
    "lint": "eslint --color \"./{src,__tests__,scripts}/**/*.{cjs,js,jsx,mjs,ts,tsx}\"",
    "lint-fix": "yarn lint --fix",
    "prettify": "prettier --color --write \"**/{.*/,}*.{cjs,css,htm,html,js,json,json5,jsx,md,mjs,scss,ts,tsx,vue,yaml,yml}\" \"!**/test-fixtures/**\"",
    "test": "yarn lint"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@types/eslint": "8.4.6",
    "@types/micromatch": "4.0.2",
    "@willbooster/prettier-config": "^9.0.1",
    "eslint": "8.25.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-class-members": "1.15.2",
    "eslint-plugin-sort-destructure-keys": "1.4.0",
    "eslint-plugin-unicorn": "43.0.2",
    "lint-staged": "13.0.3",
    "micromatch": "4.0.5",
    "prettier": "2.7.1",
    "sort-package-json": "2.0.0"
  },
  "peerDependencies": {
    "@willbooster/prettier-config": ">=6",
    "eslint": ">=8",
    "eslint-config-prettier": ">=8",
    "eslint-plugin-import": ">=2",
    "eslint-plugin-sort-class-members": ">=1.14",
    "eslint-plugin-sort-destructure-keys": ">=1.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
