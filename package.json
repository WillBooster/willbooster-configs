{
  "name": "@willbooster/configs",
  "version": "0.0.0-semantically-released",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/WillBooster/willbooster-configs.git"
  },
  "license": "Apache-2.0",
  "author": "WillBooster Inc.",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "cleanup": "yarn format && yarn lint-fix",
    "format": "sort-package-json && yarn prettify && yarn workspaces foreach --parallel --verbose run format",
    "postinstall": "husky install",
    "lint": "yarn workspaces foreach --parallel --verbose run lint",
    "lint-fix": "yarn workspaces foreach --parallel --verbose run lint-fix",
    "prepack": "pinst --disable",
    "postpack": "pinst --enable",
    "prettify": "prettier --cache --color --write \"**/{.*/,}*.{cjs,css,cts,htm,html,js,json,json5,jsx,md,mjs,mts,scss,ts,tsx,vue,yaml,yml}\" \"!**/packages/**\" \"!**/test-fixtures/**\"",
    "release": "yarn multi-semantic-release --debug",
    "test": "CI=1 FORCE_COLOR=3 yarn workspaces foreach --verbose run test",
    "typecheck": "yarn workspaces foreach --parallel --verbose run typecheck"
  },
  "prettier": "@willbooster/prettier-config",
  "devDependencies": {
    "@qiwi/multi-semantic-release": "6.7.0",
    "@types/eslint": "8.40.2",
    "@types/micromatch": "4.0.2",
    "@typescript-eslint/parser": "5.60.1",
    "@willbooster/prettier-config": "^9.0.1",
    "conventional-changelog-conventionalcommits": "6.1.0",
    "dotenv-cli": "7.2.1",
    "eslint": "8.44.0",
    "husky": "8.0.3",
    "lint-staged": "13.2.3",
    "micromatch": "4.0.5",
    "pinst": "3.0.0",
    "prettier": "2.8.8",
    "sort-package-json": "2.5.0",
    "typescript": "5.1.6"
  },
  "packageManager": "yarn@4.0.0-rc.47"
}
